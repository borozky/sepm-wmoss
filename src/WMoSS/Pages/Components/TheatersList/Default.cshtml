@model IEnumerable<Entities.Theater>

<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Theaters <span class="caret"></span></a>
    <ul class="dropdown-menu">
        <li><a asp-page="/Theaters/Index">All Theaters</a></li>
        <li role="separator" class="divider"></li>
        @foreach(var theater in Model)
        {
            <li><a asp-page="/Theaters/Details" asp-route-id="@theater.Id">@theater.Name</a></li>
        }
    </ul>
</li>